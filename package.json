{
    "name": "pride",
    "description": "Ayano's pride pfp generator! (:",
    "version": "1.0.0",
    "main": "dist/index.js",
    "types": "index.d.ts",
    "repository": "https://github.com/ayanobot/pride.git",
    "author": "newt! <newtykins@outlook.com>",
    "license": "MIT",
    "packageManager": "yarn@3.0.2",
    "scripts": {
        "build": "tsc",
        "docs": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/index.yml --output docs/docs.json",
        "docs:test": "docgen --jsdoc jsdoc.json --source src/*.ts src/**/*.ts --custom docs/index.yml",
        "stage": "npm run docs && git add ."
    },
    "dependencies": {
        "jimp": "^0.16.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/preset-env": "^7.15.6",
        "@babel/preset-typescript": "^7.15.0",
        "@discordjs/docgen": "discordjs/docgen#ts-patch",
        "@types/babel__core": "^7",
        "@types/babel__preset-env": "^7",
        "@types/jimp": "^0.2.28",
        "@types/node": "^16.10.2",
        "jsdoc-babel": "^0.5.0",
        "pre-commit": "^1.2.2",
        "typescript": "^4.4.3"
    },
    "precommit": [
        "stage"
    ],
    "keywords": [
        "ayano",
        "bot",
        "discord",
        "canvas",
        "image",
        "level"
    ]
}
